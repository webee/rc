darwin* env/profile /etc/profile env/profile_after.sh
l-* env/python.corp.sh /etc/profile.d/python.sh env/python_after.sh
!l-* env/python.sh /etc/profile.d/python.sh env/python_after.sh
* env/pip.conf ~/.pip/pip.conf
* ssh/config_head ~/.ssh/config_head
* ssh/config_tail ~/.ssh/config_tail
!l-* env/gitconfig ~/.gitconfig
!l-* var/tmux.conf ~/.tmux.conf
!l-* scripts/mytmux.sh ~/scripts/mytmux.sh
!l-* scripts/sshsocks.sh ~/scripts/sshsocks.sh
